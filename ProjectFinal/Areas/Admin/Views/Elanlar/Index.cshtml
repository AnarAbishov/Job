@model IEnumerable<ProjectFinal.Models.Elanlar>

@{
    ViewBag.Title = "Elanlar";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<p>
    @Html.ActionLink("Create New", "Create", "Elanlar", null, new { @class = "btn btn-primary" })
</p>
<table class="table table-responsive" id="paginate">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Telefon)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Shirket_adi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ish_barede_melumat)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Status)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
    <tr class="tr">
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Telefon)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Shirket_adi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ish_barede_melumat)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
            @Html.ActionLink("Details", "Details", new { id = item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.Id })
        </td>
    </tr>
    }

</table>
<div class="row">
    <div id='page_navigation'>

    </div>
</div>
