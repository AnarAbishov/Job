
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="parallax_elan">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="title">
                    <h2>@ViewBag.elan.Kategoriya.Ad : </h2>
                    <h2>@ViewBag.elan.Vezife.Ad</h2>
                    <br>
                    <h3 class="company">@ViewBag.elan.Shirket_adi - </h3>
                    <h3 class="maash">@ViewBag.elan.Min_maash.Mebleg - @ViewBag.elan.Max_maash.Mebleg</h3>
                </div>
            </div>
            <div class="col-md-4">
                <div class="parametrler">
                    <ul>
                        <li>
                            <h5>Şəhər : </h5>
                            <h6>@ViewBag.elan.Sheher.Ad</h6>
                        </li>
                        <li>
                            <h5>Yaş : </h5>
                            <h6>@ViewBag.elan.Min_yashlar.Min_yash - @ViewBag.elan.Max_yashlar.Max_yash</h6>
                        </li>
                        <li>
                            <h5>Təhsil : </h5>
                            <h6>@ViewBag.elan.Tehsil.Derecesi</h6>
                        </li>
                        <li>
                            <h5>İş təcrübəsi : </h5>
                            <h6>@ViewBag.elan.Ish_tecrubesi.Ili</h6>
                        </li>
                        <li>
                            <h5>Elanın tarixi : </h5>
                            <h6>@ViewBag.elan.Elanin_tarixi</h6>
                        </li>
                        <li>
                            <h5>Bitmə tarixi : </h5>
                            <h6>@ViewBag.elan.Bitme_tarixi</h6>
                        </li>
                        <li>
                            <h5>Əlaqədar şəxs : </h5>
                            <h6>@ViewBag.elan.Elaqedar_shexs</h6>
                        </li>
                    </ul>
                </div>
            </div>
            <div class="col-md-4">
                <div class="elaqe">
                    <ul>
                        <li>
                            <h5>Telefon : </h5>
                            <h6>@ViewBag.elan.Telefon</h6>
                        </li>
                        <li>
                            <h5>E-mail : </h5>
                            <h6>@ViewBag.elan.Email</h6>
                        </li>
                    </ul>
                </div>
            </div>
            <div class="col-md-8">
                <div class="melumat">
                    <ul>
                        <li>
                            <h6>İş barədə məlumat</h6>
                            <p>
                                @Html.Raw(ViewBag.elan.Ish_barede_melumat)
                            </p>
                        </li>
                    </ul>
                </div>
            </div>

            <div class="col-md-8">
                <div class="telebler">
                    <ul>
                        <li>
                            <h6>Namizədə tələblər</h6>
                            <p>
                                @Html.Raw(ViewBag.elan.Namizede_telebler)
                            </p>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <div id="disqus_thread"></div>
                <script>

                    /**
                    *  RECOMMENDED CONFIGURATION VARIABLES: EDIT AND UNCOMMENT THE SECTION BELOW TO INSERT DYNAMIC VALUES FROM YOUR PLATFORM OR CMS.
                    *  LEARN WHY DEFINING THESE VARIABLES IS IMPORTANT: https://disqus.com/admin/universalcode/#configuration-variables*/
                    /*
                    var disqus_config = function () {
                    this.page.url = PAGE_URL;  // Replace PAGE_URL with your page's canonical URL variable
                    this.page.identifier = PAGE_IDENTIFIER; // Replace PAGE_IDENTIFIER with your page's unique identifier variable
                    };
                    */
                    (function () { // DON'T EDIT BELOW THIS LINE
                        var d = document, s = d.createElement('script');
                        s.src = 'https://isaxtar-az.disqus.com/embed.js';
                        s.setAttribute('data-timestamp', +new Date());
                        (d.head || d.body).appendChild(s);
                    })();
                </script>
                <noscript>Please enable JavaScript to view the <a href="https://disqus.com/?ref_noscript">comments powered by Disqus.</a></noscript>
            </div>
        </div>
    </div>
</section>


